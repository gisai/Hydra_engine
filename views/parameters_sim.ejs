<div class="panel panel-default" id="<%= (sim.name).toLowerCase()%>">
  <div class="panel-heading">
    <h3 class="panel-title"><%= sim.name%> parameters</h3>
  </div>
  <div class="panel-body form-group form-inline">
    <label for="inputEmail3" class="col-sm-2 control-label"><%= sim.name%> URL</label>
    <div class="col-sm-10">
      http://
      <input type="text" class="form-control" id="inputLocalhost" placeholder="localhost" value="<%= sim.host %>">
      :
      <input type="number" class="form-control" id="inputPort" placeholder="port" value="<%= sim.port %>">
      /
      <input type="text" class="form-control" id="inputPath" placeholder="path" value="<%= sim.path.substr(1) %>">
      <button type="button" class="btn btn-default">Check Connection</button>
    </div>
  </div>
</div>